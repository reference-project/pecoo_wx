	max-height: 100px;
}
.orderADiv2{
	-webkit-box-flex: 1;
	padding-left: 8px;
	font-size: 13px;
	color: #545454;
	line-height: 25px;
}
.orderADiv2 p.orderADiv2Lot{
	display:-webkit-box;
	overflow:hidden;
	word-break:break-all;
	-webkit-box-orient:vertical;
	-webkit-line-clamp:1;
	font-size: 15px;
	color: #333333;
}
.orderADiv2P2 span{
	color: #B10000;
}
.myBid img{
	width: 25px;
