.listFix{
	display: -webkit-box;
	position: fixed;
	width: 100%;
	top:0;
	height: 45px;
	border-bottom: 1px solid #E2E2E2;
	line-height: 45px;
	font-size:14px;
	background: #fff;
	z-index: 100;
}
.listFix ul{
	display: -webkit-box;
	width: 100%;
}
.listFix ul li{
	-webkit-box-flex:0.8;
	text-align: center;
	color: #545454;
	font-size: 14px;
}
.listFix ul li.active{
	color:#24988b;
}
.listFix ul li.active span{
	background: url(https://www.pecoo.com/pecooh5/img/mine/sanActive.png) no-repeat center center;
	width: 20px;
	height: 45px;
	display: inline-block;
	vertical-align: middle;
	background-size: 12px;
}
.listPrice span,.listCreated span{
	background: url(https://www.pecoo.com/pecooh5/img/list/san.png) no-repeat center center;
	width: 20px;
	height: 16px;
	display: inline-block;
	vertical-align: middle;
	background-size: 10px;
}
.listPrice image{
  width: 10px;
  height: 14px;
  vertical-align: middle;
}
.listDiqu image{
  width: 10px;
  height: 7px;
  vertical-align: middle
}
.listDiqu span{
	background: url(https://www.pecoo.com/pecooh5/img/list/san1.png) no-repeat center center;
	width: 20px;
	height: 45px;
	display: inline-block;
	vertical-align: middle;
	background-size: 10px;
}
.listZhTai span{
	background: url(https://www.pecoo.com/pecooh5/img/mine/san.png) no-repeat center center;
	width: 20px;
	height: 45px;
	display: inline-block;
	vertical-align: middle;
	background-size: 10px;
}
.listChange{
	background: url(https://www.pecoo.com/pecooh5/img/list/listSmall.png) no-repeat center center;
	width: 40px;
	background-size: 16px;
}
.listMenu{
	background: #FFFFFF;
	position: fixed;
	top:46px;
	width: 100%;
	z-index: 100;
	border-bottom: 1px solid #F3F3F3;
}
.listMenuUl1{
	width: 98%;
	margin: 10px auto;
	max-height: 140px;
	overflow: auto;
	margin-bottom: 15px;
}
.listMenuUl1 li{
	width: 16%;
	margin: 5px 1%;
	text-align: center;
	height: 30px;
	background: #F3F3F3;
	color: #666666;
	line-height: 30px;
	font-size: 13px;
	float: left;
	overflow: hidden;
	padding: 0 1%;
}
.listMenuUl1 li.active{
	background: #24998B;
	color: #FFFFFF;
}

