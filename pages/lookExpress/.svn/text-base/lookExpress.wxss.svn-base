page{
	background: #F3F3F3;
}
div,p,ul,li{
  display: block;
}
.expressTitle{
	margin: 15px 0 10px 0;
	font-size: 14px;
	color: #0db789;
	text-indent: 10px;
	border-left: 5px solid #0DB789;
}
.expressState{
	padding: 10px 15px 10px 15px;
	background: #FFF;
	font-size: 13px;
	color: #CCCCCC;
}
.expressState>div{
	width: 25%;
	text-align: center;
	float: left;
	line-height: 25px;
}
.expressState2>div{
	width: 33.333%;
	text-align: center;
	float: left;
	line-height: 25px;
}
.expressState>div>p>span{
	width: 20px;
	height: 12px;
    display: block;
    margin: 8px auto 0 auto;
	background: #FFFFFF;
	position: relative;
	z-index: 2;
}
.expressState>div>p>span>span{
	width: 10px;
	height:10px;
	border-radius: 50%;
	display:block;
	margin: 5px auto 0 auto;
	background: #CCCCCC;
}
.expressState>div>span{
	color: #333333;
}
.expressState>p.expressHr{
	height: 2px;
	width: 68%;
	background: #CCCCCC;
	left: 16%;
	top: 47px;
	z-index: 1;
}
.expressState>p.expressHr2{
	height: 2px;
	width: 64%;
	background: #CCCCCC;
	left: 18%;
	top: 47px;
	z-index: 1;
}
.expressState>div.active>p>m{
    padding: 2px 14px;
    color: #FFFFFF;
    background: url(https://www.pecoo.com/pecooh5/img/mine/expressBg.png) no-repeat center center;
    padding-bottom: 10px;
    background-size: 100%;
    width: 100%;
}
.expressState>div.active>p>m.m2{
	padding: 2px 3px;
    color: #FFFFFF;
    background: url(https://www.pecoo.com/pecooh5/img/mine/expressBg.png) no-repeat center center;
    padding-bottom: 10px;
    background-size: 100%;
    width: 100%;
}
.expressState>div.active>p>m.m3{
	padding: 3px 18px;
    color: #FFFFFF;
    background: url(https://www.pecoo.com/pecooh5/img/mine/expressBg.png) no-repeat center center;
    padding-bottom: 10px;
    background-size: 100%;
    width: 100%;
}
.expressState>div.active>p>span>span{
	width: 10px;
	height:10px;
	border-radius: 50%;
	display:block;
	margin: 5px auto 0 auto;
	background: #0DB789;
}
.lookState{
	background: #FFFFFF;
}
.lookStateDiv{
	position: relative;
	font-size: 15px;
	color: #999999;
	padding:15px;
	display: -webkit-box;
	border-bottom: 1px solid #F3F3F3;
	line-height: 20px;
}
.lookStateDiv p.lookStatePLeft{
	text-align: center;
	padding-right: 15px;
}
.lookStateDiv p.lookStatePLeft span:last-child{
	font-size: 12px;
}
.lookStateDiv p.lookStateP{
	-webkit-box-flex: 1;	
}
.lookStateDiv p.lookStateImg{
	width: 25px;
	text-align: right;
}
.lookStateDiv p.lookStateImg image{
	width: 15px;
   height: 8px;
	margin-top: 15px;
}
.lookStateDiv>span{
	position: absolute;
    width: 23%;
    height:20px;
    left: 0;
    bottom: -9px;
    z-index: 54;
    background: #fff;
}
.lookStateDiv>span>m{
	width: 1px;
	height: 20px;
	background: #DDDDDD;
	position: relative;
	z-index: 100;
	display: block;
	margin-left: 45px;
}

.expressDiv{
	background: #F3F3F3;
	padding: 10px 0px 0 35px;
}
.expressDiv li{
	display: -webkit-box;
}
.expressDiv li>span{
	background: url(https://www.pecoo.com/pecooh5/img/list/expressHui.png) no-repeat top center;
	display:block;
	background-size: 50%;
	width:20px;
	margin-right: 20px;
}
.expressDiv li:last-child>span{
	background: url(https://www.pecoo.com/pecooh5/img/list/expressHui.png) no-repeat top center;
	display:block;
	background-size: 50%;
	width:20px;
	margin-right: 20px;
	height: 10px;
}

.lookStateUl li:nth-child(1) li:first-child span{
	background: url(https://www.pecoo.com/pecooh5/img/list/expressBlue.png) no-repeat top center;
	display:block;
	background-size: 70%;
	width:20px;
	margin-right: 20px;
	height: auto;
}
.expressDivD{
	-webkit-box-flex: 1;
	line-height: 24px;
	border-bottom: 1px solid #DDDDDD;
	color: #bbbbbb;
	padding-bottom: 8px;
	margin-bottom: 8px;
	padding-right: 15px;
}

.expressDiv  li:last-child div.expressDivD{
	border-bottom: none;
}
.expressDivD>p:first-child{
	font-size: 14px;
	line-height: 20px;
	padding-top: 2px;
}
.expressDivD>p:last-child{
	font-size: 12px;
}
.lookStateUl li:last-child div.lookStateDiv>span{
	display: none;
}
.lookStateUl li:first-child div.lookStateDiv{
	color: #333333;
}
.lookStateUl li:first-child p.lookStatePLeft span:last-child{
	color: #999999;
}
.expressFly{
	padding: 5px 8px;
    color: #FFFFFF;
    background: url(https://www.pecoo.com/pecooh5/img/mine/expressBg.png) no-repeat center center;
    padding-bottom: 10px;
    background-size: 50px;
    width:50px;
    right: 18%;
    top: 12px;
    text-align: center;
}
