.swiper{
  width: 100%;
  height: 280px;
}
.slide-image {
  width: 100%;
}

.selectGood{
	padding: 10px 0;
	box-shadow: 0 0 5px #F3F3F3;
}
.selectGoodP{
	font-size: 18px;
	padding-bottom: 5px;
	line-height: 30px;
	padding:0 5px 0px 0px;
	border-left: 5px solid #24998B;
	text-indent: 10px;
}

/*LV*/
.logoDiv{
	padding: 10px 0;
	background: #F3F3F3;
}
.saleUlDiv3{
	display: -webkit-box;
	background: #FFFFFF;
	box-shadow:0 0 5px #F3F3F3;
	padding: 1px 0;
}
.saleUlDiv31{
	-webkit-box-flex: 1.0;
	height: 251px;
	border-right: 1px solid #F3F3F3;
}

.saleUlDiv31 navigator{
	display: table-cell;
	vertical-align: middle;
	text-align: center;
	height: 251px;
	width: 500px;
}
.saleUlDiv31 navigator image{
	width: 100%;
	height: 100%;
	display: block;
	margin:0 auto;
}
.saleUlDiv32{
	width: 125px;
}
.saleUlDiv32 navigator{
	display: table-cell;
	width: 125px;
	height: 125px;
	text-align: center;
	vertical-align: middle;
}
.saleUlDiv32 view:first-child{
	border-bottom: 1px solid #F3F3F3;
}
.saleUlDiv32 navigator image{
	max-width:125px;
	max-height: 125px;
	display: block;
	margin:0 auto;
}