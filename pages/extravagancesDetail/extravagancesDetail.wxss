  line-height: 70rpx;
}
.shop{
  margin-top: 50rpx;
  width: 100%;
  height: 110rpx;
  text-align: center;
  line-height: 110rpx;
  font-size: 16px;
  background: #111111;
  color: #FFFFFF;
}
.selected{
  border-color: #1B1B1B !important;
}
.triangle{
  position: absolute;
  right: 0;
  bottom: 0;
  width: 0px;
  height: 0px;
  border-width: 4px;
  border-style: solid;
  border-color: transparent #1B1B1B #1B1B1B transparent;
}
.left-triangle{
  border:107rpx solid transparent;
  border-right: 0px solid transparent;
  border-bottom: 64rpx solid #CCCCCC;
  border-top: 0px solid transparent;
  position: absolute;
  right: 0;
  bottom: 0;
  opacity: .4;
}
.right-triangle{
  border:107rpx solid transparent;
  border-top: 64rpx solid #CCCCCC;
  border-bottom:0px solid transparent;
  position:absolute;
  left:0;
  bottom:0;
  opacity: .4;
  border-left:0px solid transparent;
}