/* 弹框 */
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background-color: rgba(0,0,0,.5);
  overflow: hidden;
}
.mask-content{
  width: 100%;
  overflow: hidden;
  position: fixed;
  height: 980rpx;
  bottom: 0;
  left: 0;
  z-index: 1001;
  background: #fff;
}
.scroll{
  overflow-y:scroll;
  height: 540rpx;
}
.bottom-pos{-webkit-transform:translateY(100%);transform:translateY(100%);}
.pro-title{
  padding: 40rpx 30rpx;
  display: flex;
}
.pro-default-img{
  width: 200rpx;
  height: 200rpx;
  margin-right: 30rpx;
  border: 1rpx solid #DDDDDD;
