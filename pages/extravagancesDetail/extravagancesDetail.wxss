}
.borders{
  border-color: #757575;
}
.add{
  color: #757575;
  float:left;
  border: 1rpx solid #757575;
}
.digital{
  float: left;
  width: 180rpx;
  height: 70rpx;
  border: 1rpx solid #757575;
  border-right: none;
