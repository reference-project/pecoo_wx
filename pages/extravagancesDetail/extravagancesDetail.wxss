  overflow: hidden;
  font-family: 'PingFang-SC-Regular';
}
.item-color, .item-size{
  min-width: 88rpx;
  padding: 0 10rpx;
  height: 62rpx;
  border: 1px solid #CCCCCC;
  margin: 10rpx 20rpx 0 0;
  font-size: 14px;
  text-align: center;
  line-height: 62rpx;
  float: left;
  position: relative;
}
.pro-number{
  padding: 20rpx 30rpx;
  z-index: 99;
  font-size: 15px;
}
.pro-btn-box{
  height: 72rpx;
  line-height: 72rpx;
  margin-top: 10rpx;
  padding-left: 30rpx;
}
.btns{
  display: inline-block;
  width: 90rpx;
