page{
  width: 100%;
  background: #F6F8FA;
}
/* 轮播图样式 */
.swiperTime{
	position: absolute;
	top:0;
	z-index: 200;
	background: rgba(255,255,255,.6);
	width: 100%;
	/* height: 25px; */
	color: #DB1014;
	font-size: 14px;
	text-align: center;
	/* line-height: 25px; */
}
.swiper-container {
  width: 100%;
  position: relative;
}
swiper {
  text-align: center;
  font-size: 18px;
  background: #fff;
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  width:100%;
  height: 330px;
}
swiper image{
  width: 100%;
  height: 650rpx;
	max-width:100%;
	max-height: 650rpx;
}
.swiper-pagination-bullet{
	background: #666;
	width:5px;
	height:5px;
}
.swiper-container-horizontal>.swiper-pagination {
  bottom: 10px;
  left: 0;
  width: 100%;
  height: 15px;
  overflow: hidden;
}
#myAudio{
  display: none;
}
/* 轮播底下内容 */
.collection{
  width: 47rpx;
  height: 40rpx;
}
.pro-content{
  height: 525rpx;
  background: #FFFFFF;
  padding:0 30rpx;
}
.pro-price{
  padding-top: 40rpx;
  display: flex;
  height: 50rpx;
  justify-content: space-between;
}
.pro-money{
  font-size: 14px;
}
.pro-num{
  font-size: 24px;
}
.pro-words{
  width: 70rpx;
  margin-top: 16rpx;
  text-align: center;
  background: #E83E3E;
  color: #FFFFFF;
  font-size: 10px;
}
.pro-today{
  margin-top: 20rpx;
  display: flex;
  font-family: 'PingFang SC';
  justify-content: space-between;
  font-size: 13px;
  color: #999999;
}
.pro-currencies{
  color: #999999;
  font-family: 'PingFang-SC-Medium';
}
.pro-name{
  font-family: 'PingFang-SC-Medium';
  color: #333333;
  font-size: 19px;
  margin-top: 50rpx;
}
.pro-from{
  margin-top: 34rpx;
  color: #666666;
  font-family: 'PingFang SC';
  font-size: 14px;
}
.pro-expect{
  font-family: 'PingFang-SC-Regular';
  color: #333333;
  font-size: 14px;
}
.pro-tips{
  font-family: 'PingFang-SC-Regular';
  color: #999999;
  font-size: 14px;
}
/* 颜色/尺寸/数量 */
.pro-appearance{
  margin-top: 20rpx;
  padding: 0 30rpx;
  height: 140rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: 'PingFang-SC-Medium';
  color: #333333;
  background: #FFFFFF;
  font-size: 15px;
}
.already-selected{
  padding-right: 40rpx;
}
/* 快递流程 */
.pro-process{
  margin: 20rpx 0;
  padding: 0 30rpx;
  height: 270rpx;
