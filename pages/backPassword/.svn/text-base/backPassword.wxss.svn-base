page{
	background: #F0F0F0;
}
div,p,ul,li{
  display: block;
}
.regLogin{
	right: 2%;
	top:0;
	font-size: 16px;
	color:#666666;
}
section{
  display: block;
	width:90%;
	margin:20px auto;
}
section span{
	font-size:15px;
}
section>div{
	height: 44px;
	line-height: 44px;
	width: 100%;
	margin-bottom: 12px;
	position: relative;
}
.uesrDiv{
	display: -webkit-box;
	border-bottom:1px solid #ddd;
}
.testingDiv{
	display: -webkit-box;
}
.testing{
	background:#f0f0f0;
}
.passwordDiv{
	display: -webkit-box;
	border-bottom: 1px solid #dddddd;
}
.uesrDiv input{
	-webkit-box-flex: 1;
	font-size: 14px;
	background: #f0f0f0;
	display: block;
	line-height: 43px;
	padding-left: 5px;
  padding-top: 10px;
}
.testingDivS1{
	-webkit-box-flex: 1;
	display:-webkit-box;
	border-bottom: 1px solid #dddddd;
}
.testingGet{
  display: block;
	background: #24998B;
	width: 105px;
	height: 44px;
	text-align: center;
	position: absolute;
	right: 0;
	top: 0;
  z-index: 100;
	line-height: 44px;
	color: #FFFFFF;
	border-left: 10px solid #f0f0f0;
}
#picCodeImg{
	width: 105px;
	height: 44px;
}
.uesrDiv span,.passwordDiv span,.testingDivS1 span{
	width: 60px;
	text-indent: 10px;
	display: inline-block;
}
section>div input.password{
	-webkit-box-flex: 1;
	display: block;
	background: #f0f0f0;
	font-size: 15px;
	line-height: 44px;
  padding-top: 10px;
}
.testingDivS1>input{
	display: block;
	line-height: 44px;
	background: none;
	-webkit-box-flex: 1;
  padding-top: 10px;
}
.next,.register{
	background: #24998B;
	color: #FFFFFF;
	margin:30px auto 10px auto;
	height: 44px;
	text-align: center;
	line-height: 44px;
	font-size:18px;
	border-radius: 2px;
}
.next{
	margin-top:48px;
}
.testingGet2{
	background: #D5D5D5;
	width: 105px;
    height: 44px;
    text-align: center;
    position: absolute;
    right: 0;
    top: 0;
    line-height: 44px;
  	color: #ffffff;
    border-left: 10px solid #f0f0f0;
}
#b1,#b2,#b3{
	display: none;
	font-size: 16px;
	color: #333;
	line-height: 40px;
	text-align: center;
}
#errorRegister{
	font-size: 16px;
	color: #333;
	line-height: 20px;
	text-align: center;
}
#codeMeaasge{
	line-height:40px;
  font-size: 14px;
}
.agreement input{
	width: 16px;
	height: 16px;
	border:2px solid #DDDDDD;
	-webkit-appearance: checkbox;
	vertical-align: middle;
}
.agreement{
	width: 90%;
	margin:0 auto;
	font-size: 14px;
	color: #545454;
	float: left;
	display: block;
}
/*ע���ɹ�*/
.regSuccess{
	width: 70%;
	height: 200px;
	z-index: 500;
	background: #FFFFFF;
	border-radius: 5px;
}