.swiperTime{
	position: absolute;
	top:0;
	z-index: 200;
	background: rgba(255,255,255,.6);
	width: 100%;
	height: 25px;
	color: #DB1014;
	font-size: 14px;
	text-align: center;
	line-height: 25px;
}
.swiper-container {
    width: 100%;
    position: relative;
}
swiper {
    text-align: center;
    font-size: 18px;
    background: #fff;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    width:100%;
	  height: 330px;
}
swiper image{
  width: 100%;
  height: 330px;
	max-width:100%;
	max-height: 330px;
}
.swiper-pagination-bullet{
	background: #666;
	width:5px;
	height:5px;
}
.swiper-container-horizontal>.swiper-pagination {
    bottom: 10px;
    left: 0;
    width: 100%;
    height: 15px;
    overflow: hidden;
}
