/* pages/RealName/RealName.wxss */
page{
  background: #F6F8FA;
  width: 100%;
  height: 100%;
  font-size: 15px;
}
.box{
  margin-top: 20rpx;
  width: 100%;
  height: 100%;
  background: #FFFFFF;
}
.content>view{
  padding: 0 30rpx;
  display: flex;
  height: 50px;
  align-items: center;
  border-bottom: 1rpx solid #dddddd;
}
.label{
  width: 160rpx;
}
.code{
  width: 200rpx;
  height: 60rpx;
  margin-right: 40rpx;
  text-align: center;
  line-height: 60rpx;
  background: #1A191E;
  color: #FFFFFF;
}
.confirm{
  margin-top: 100rpx;
  margin-right: 20px;
  display: block !important;
  border-bottom: none !important;
}
.submit{
  width: 100%;
  height: 50px;
  background: #1A191E;
  color: #FFFFFF;
  text-align: center;
  line-height: 50px;
}
.words{
  font-size: 14px;
  margin-top: 10rpx;
  color: #999999;
}
.bg-col{
  background: #FFFFFF;
  color: #E83E3E;
}
.content-view{
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
}
.content-words{
  color: #999999;
}