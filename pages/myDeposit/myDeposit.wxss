page{
  background: #F3F3F3;
}
.voucher-list{
  margin-bottom: 130rpx;
}
.voucher-head{
  display: flex;
  justify-content: space-around;
  height: 60px;
  line-height: 60px;
  background: white;
  font-family: 'MicrosoftYaHei';
  font-size: 14px;
  color: #666666;
}
.voucher-head view{
  height: 50px;
}
.active{
  color: #D74A42;
  border-bottom: 2px solid #D74A42;
}
.use{
  width: 160rpx;
  height: 60rpx;
  background: #DA5C55;
  border-radius: 30rpx;
  font-size: 13px;
  color: #FFFFFF;
  text-align: center;
  line-height: 60rpx;
  margin-left: 60rpx;
}
.voucher-money{
  height: 236rpx;
  margin: 20rpx 20rpx 0;
  background: #F3F3F3;
  position: relative;
  z-index: 1;
  overflow: hidden;
}
.item-top{
  background-color: #FFFFFF;
  /* background-image: url() */
  border-bottom-left-radius: 15px;
  border-bottom-right-radius: 15px;
  height: 160rpx;
  display: flex;
  align-items: center;
}
.code{
  font-size: 15px;
  color: #D74A42;
  margin: 36rpx 6rpx 0 39rpx;
}
.actual-money{
  font-size: 30px;
  color: #D74A42;
  margin-top: 10rpx;
}
.actual-detail{
  margin: 30rpx 0 20rpx 38rpx;
  font-size: 15px;
  color: #333333;
  font-family: 'MicrosoftYaHei';
}
.private{
  color: #999999;
  font-size: 12px;
}
.item-bottom{
  display: flex;
  justify-content: space-between;
  padding-right: 60rpx;
  height: 76rpx;
  line-height: 76rpx;
  border-top-left-radius: 15px;
  border-top-right-radius: 15px;
  background: #eeeeee;
  padding-left: 39rpx;
  font-family: 'MicrosoftYaHei';
  font-size: 12px;
  color: #999999;
}
.exchange{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 130rpx;
  display: flex;
  z-index: 99;
  align-items: center;
  background: #FFFFFF;
}
.exchange input{
  flex: 1;
  height: 90rpx;
  margin-left: 20rpx;
  border: 1rpx solid #F3F3F3;
}
.exchange view{
  width: 200rpx;
  background: #1A191E;
  height: 90rpx;
  margin-right: 20rpx;
  line-height: 90rpx;
  font-size: 15px;
  font-family: 'MicrosoftYaHei';
  text-align:center;
  color: #FFFFFF;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}
.change-bgcol{
  opacity: .6;
}
.voucher-pic{
  width: 140rpx;
  height: 133rpx;
  position: absolute;
  right: -10rpx;
  top: -10rpx;
}