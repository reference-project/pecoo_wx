page{
	background: #FFFFFF;
  font-family: 'PingFang-SC-Medium';
  font-size: 16px;
}
div,p,ul,li{
  display: block;
}
.regLogin{
	right: 2%;
	top:0;
	font-size: 16px;
	color:#666666;
}
.back-pwd-box{
  display: block;
	width:90%;
	margin:110rpx 5% 0;
}
.back-pwd-box text{
	font-size:15px;
}
.back-pwd-box>view{
	height: 44px;
	line-height: 44px;
	width: 100%;
	border-bottom:1px solid #EEEEEE;
	margin-bottom: 60rpx;
	position: relative;
}
.uesrDiv{
	display: -webkit-box;
}
.testingDiv{
	display: -webkit-box;
}
.testing{
	background:#f0f0f0;
}
.passwordDiv{
	display: -webkit-box;
	border-bottom: 1px solid #dddddd;
}
.uesrDiv input{
	-webkit-box-flex: 1;
	font-size: 14px;
	background: #f0f0f0;
	display: block;
	line-height: 43px;
	padding-left: 5px;
  padding-top: 10px;
}
.testingDivS1{
	-webkit-box-flex: 1;
	display:-webkit-box;
}
.testingGet{
  display: block;
	background: #1B1B1B;
	text-align: center;
	position: absolute;
	right: 0;
	top: 0;
  z-index: 100;
	color: #FFFFFF;
}
#picCodeImg{
	width: 200rpx;
	height: 80rpx;
}
.uesrDiv text,.passwordDiv text,.testingDivS1 text{
	width: 60px;
	text-indent: 10px;
	display: inline-block;
}
.password{
	-webkit-box-flex: 1;
	display: block;
	font-size: 15px;
	line-height: 44px;
  padding-top: 10px;
}
.testingDivS1>input{
	display: block;
	line-height: 44px;
	background: none;
	-webkit-box-flex: 1;
  padding-top: 10px;
}
.next,.register{
	background: rgba(27,27,27,1);
	color: #FFFFFF;
	margin-top:30px;
	height: 44px;
	text-align: center;
	line-height: 44px;
	font-size:18px;
	border-radius: 2px;
}
.next{
	margin-top:48px;
}
.testingGet2{
	background: #D5D5D5;
  height: 44px;
  text-align: center;
  position: absolute;
  right: 0;
  top: 0;
  line-height: 44px;
  color: #ffffff;
}
#b1,#b2,#b3{
	display: none;
	font-size: 16px;
	color: #333;
	line-height: 40px;
	text-align: center;
}
#errorRegister{
	font-size: 16px;
	color: #333;
	line-height: 20px;
	text-align: center;
}
#codeMeaasge{
	line-height:40px;
  font-size: 14px;
}
.agreement input{
	width: 16px;
	height: 16px;
	border:2px solid #DDDDDD;
	-webkit-appearance: checkbox;
	vertical-align: middle;
}
.agreement{
	width: 90%;
	margin:0 auto;
	font-size: 14px;
	color: #545454;
	float: left;
	display: block;
}
/*ע���ɹ�*/
.regSuccess{
	width: 70%;
	height: 200px;
	z-index: 500;
	background: #FFFFFF;
	border-radius: 5px;
}
