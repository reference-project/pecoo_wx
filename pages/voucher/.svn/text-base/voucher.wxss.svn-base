page{
  background: #F3F3F3;
}
.scoll-h{height: 100%;}
.tab-h{
    height: 44px;
    width: 100%; 
    box-sizing: border-box;
    overflow: hidden;
    line-height: 44px;
    background: #efefef; 
    font-size: 14px; 
    white-space: nowrap;
    position: fixed;
    top: 0; 
    left: 0; 
    z-index: 99;
    border-bottom: 1px solid #DDDDDD;
}
.tab-item{
  width:33.33%;
  text-align: center;
  display: inline-block;
}
.tab-item.active{
  color: #24988b;
  position: relative;
}
.tab-item.active:after{ 
  content: "";
  display: block;
  height: 8rpx;
  width: 60%;
  background: #24988b;
  position: absolute; 
  bottom: 0;
  left: 20%;
}
.item-ans{ 
  width: 100%;
  display: flex; 
  flex-grow: row no-wrap;
  justify-content: space-between; 
  padding: 30rpx;
  box-sizing: border-box; 
  height: 180rpx;
  align-items: center;
  border-bottom: 1px solid #F2F2F2;
}
.voucherUl1{
	position: fixed;
	background: #FFFFFF;
	height: 44px;
	width: 100%;
	top: 0;
	display: -webkit-box;	
	z-index:500;
	border-bottom: 1px solid #F3F3F3;
}
.voucherUl1>view{
	-webkit-box-flex: 1.0;
	font-size: 15px;
	text-align: center;
	line-height: 44px;
	color: #666666;
	margin: 0 15px;
}
.voucherUl1>view.active{
	color: #24988b;
	border-bottom: 3px solid #24988b;
}
/*����*/
.voucherUl2{
	width: 96%;
	margin:0 auto;
	margin-top: 55px;
	margin-bottom: 55px;
}
.voucherUl2>view{
	margin-bottom: 5px;
}
.voucherD{
	display: -webkit-box;
	height:100px;
	margin-bottom: 8px;
}
.voucherD input{
	-webkit-appearance: checkbox;
	width: 18px;
	height: 18px;
	vertical-align: middle;
	margin-top: 35px;
	margin-right:5px;
}
.voucherDiv{
	background:#fd5d17;
	-webkit-box-flex: 1;
	color: #FFFFFF;
	padding: 5px;
}
.voucherMoney span:first-child{
	font-size:50px;
	display: inline-block;
	vertical-align: top;
}
.voucherMoney span:last-child{
	font-size:16px;
	vertical-align:top;
	display: inline-block;
	margin-top: 10px;
}
.voucherTime{
	text-align: right;
	padding-top: 10px;
}
.lookVoucher{
	width: 90%;
	margin:0 auto;
	font-size: 14px;
	line-height: 40px;
}
.lookVoucher span{
	color: #999;
}
.lookVoucher a{
	float: right;
	color: #DB1014;
}
.voucherNone{
	padding: 20px;
	text-align: center;
	font-size: 14px;
	line-height: 25px;
}
.voucherNone img{
	width: 80px;
}
.voucherNone p{
	color: #999999;
}
.voucherNone a{
	color: #DB1014;
}

