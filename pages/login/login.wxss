page{
  font-family: 'PingFang-SC-Medium';
  font-size: 14px;
  color: #333333;
}
.active{
  border-bottom: 1rpx solid #333333;
  color: #333333;
}
.user-info{
  width: 100%;
  height: 89rpx;
  border-top: 1rpx solid #EEEEEE;
  display: flex;
  color: #999999;
  justify-content: space-around;
}
.user-info view{
  height: 80rpx;
  line-height: 80rpx;
}
.user-input{
  margin: 40rpx 70rpx 0;
  height: 99rpx;
  line-height: 99rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #EEEEEE;
}
.user-input input{
  height: 70rpx;
  min-width: 60%;
}
.verify{
  font-size: 16px;
  width: 20%;
  text-align: right;
}
.password-pic{
  width: 70rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
}
.msg-code{
  color: #333333;
  text-align: center;
}
.del{
  width: 30rpx;
  height: 30rpx;
  margin:0 auto;
}
.open-eye{
  width: 27rpx;
  height: 19rpx;
  margin:0 auto;
}
.shut-eye{
  width: 27rpx;
  height: 12rpx;
  margin:0 auto;
}
.user-submit{
  height: 88rpx;
  margin: 60rpx 70rpx 0;
  line-height: 88rpx;
}
.btn-action{
  width: 100%;
  height: 100%;
  background: #1B1B1B;
  color: #FFFFFF;
  text-align: center;
}
button{
  border-radius: 0 !important;
}
.agree{
  margin: 40rpx 70rpx 0;
  font-family: 'PingFang SC';
  color: #999999;
  display: flex;
}
.protocol{
  color: #333333;
}
/* 已有拍库账号 */
.have{
  margin: 0 70rpx;
  overflow: hidden;
}
.have input{
  height: 99rpx;
  margin-top: 30rpx;
  border-bottom: 1rpx solid #EEEEEE;
}
.forget-pwd{
  text-align:right;
  margin-top: 29rpx;
  color:#999999;
  font-size: 14px;
}
.login{
  height: 88rpx;
  line-height: 88rpx;
  margin-top: 66rpx;
}
.words{
  margin-top: 30rpx;
  color: #999999;
  font-size: 12px;
}

.message{
  padding: 5px 10px;
  font-size: 12px;
  color: #999999;
  line-height: 24px;
  background: #f3f3f3;
}
.message span{
  padding:7px 5px;
  background: #ffffff;
  border-radius: 50%;
}
.message image{
  width: 20px;
  height: 20px;
  vertical-align: middle;
  margin-bottom: 4px;
}