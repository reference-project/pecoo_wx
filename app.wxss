/**app.wxss**/
/* 一行显示省略号 */
.warp{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
/* 两行显示省略号 */
.line{
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
/* 右箭头 */
.right_arrow{
  width: 12rpx;
  height: 22rpx;
}
/* 微信icon */
.wx-pic{
  width: 60rpx;
  height: 60rpx;
}
/* 搜索icon */
.search-icon-pic{
  width: 44rpx;
  height: 44rpx;
}
/* 搜索框 */
.search-view{
  border: 1rpx solid #EEEEEE;
  background: #F8F8F8;
  line-height: 60rpx;
  color: #999999;
  font-size: 14px;
  border-radius:4rpx;
  font-family: 'PingFang-SC-Regular'; 
}
.computing{
  color: #E83E3E;
  background: #FFFFFF;
  font-size: 14px;
  text-align: center;
  min-width: 100rpx;
  /* float: right; */
}
/* 选中图片 */
.checked-pic{
  width: 40rpx;
  height: 40rpx;
}
/* 主题色 */
.theme-col{
  color: #D74A42;
}
/* 暂无订单/收藏 */
.not-box{
  width: 100%;
  margin-top: 279rpx;
  text-align: center;
}
.not-pic{
  width: 240rpx;
  height: 240rpx;
  margin: 0 auto;
}
.not-pic image{
  width: 100%;
  height: 100%;
  margin: 0 auto;
}
.not-words{
  font-family: 'PingFang-SC-Medium';
  font-size: 14px;
  color: #BBBBBB;
}
.btn{
  width: 340rpx;
  height: 80rpx;
  border: 1rpx solid #1B1B1B;
  text-align: center;
  line-height: 80rpx;
  margin: 0 auto;
  font-size: 15px;
  margin-top: 60rpx;
}
/*详情页的遮罩*/
.shade-black{
	width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background-color: rgba(0,0,0,.5);
  overflow: hidden;
}
/* 解决高度塌陷 */
.overflow{
  overflow: hidden;
}
/* 边距色 */
.step-style{
  width: 100%;
  height: 20rpx;
  background: #F6F8FA;
}
/* 隐藏 */
.hidden{
  display: none;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.dataNone{
  text-align: center;
  line-height: 30px;
  color: #666;
  font-size: 14px;
}
.clear:after{
  content: "";
  display:block;
  clear:both;
  visibility:hidden;
  height:0;
  overflow: hidden;
}
.clearfix:after {
 	display: block;
	clear: both;
  content: "";
}
.fl{float:left;}
.fr{float:right;}
.abs{position:absolute;}
.rel{position:relative;}
.none{
	display: none;
}
.voucherNone image{
  margin:20px auto 10px auto;
  width: 80px;
  display:block;
  height:80px;
}
.voucherNone p{
  text-align: center;
  display: block;
  font-size: 14px;
  color: #999999;
}
.submitOrder {
  width: 90%;
  height: 45px;
  text-align: center;
  line-height: 45px;
  color: #FFFFFF;
  font-size: 16px;
  background: #1A191E;
  margin: 60px auto;
}
radio .wx-radio-input{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx;
}
radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background: #D74A42;
}
radio .wx-radio-input.wx-radio-input-checked::before{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx;
   line-height: 40rpx;
   text-align: center;
   font-size:30rpx;
   color:#fff;
   background: #D74A42;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}
checkbox .wx-checkbox-input{
   width: 35rpx;
   height: 35rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
   border: none;
   background: #D74A42;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
   width: 35rpx;
   height: 35rpx;
   line-height: 35rpx;
   text-align: center;
   font-size:25rpx;
   color:#fff;
   background: #D74A42;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}
.drawer_box { 
  width: 550rpx;  
  overflow: hidden;  
  position: fixed;  
  top: 60%;  
  left: 50rpx;  
  z-index: 3001;  
  background: #FAFAFA;  
  margin: -150px 50rpx 0 50rpx;  
  border-radius: 3px;  
}
.drawer_box>view{
  font-size: 18px;
  text-align: center;
  line-height: 60px;
} 
.btn_ok{ 
  font-size: 16px;  
  text-align: center;  
  color: #3CC51F;  
} 
.searchLoadingComplete{
  text-align: center;
  font-size: 16px;
  /* padding-bottom: 20px; */
}
.bold{
	font-weight: bold;
}
.underlinebold{
	text-decoration: underline;
	font-weight: bold;
}